    "version": "0.2.0",
    "configurations": [
        {
            "name": "example",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "cwd": "example",
            "toolArgs": [
                // "--web-renderer",
                // "html"
            ]
        },
        {
            "name": "example (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "cwd": "example",
            "toolArgs": [
                "--local-engine=ios_profile",
                "--local-engine-host=host_debug_unopt_arm64",
            ]
        },
        {
            "name": "example (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "cwd": "example",
            "toolArgs": [
                "--local-engine=ios_release",
                "--local-engine-host=host_debug_unopt_arm64",
            ],
        }
    ]
}